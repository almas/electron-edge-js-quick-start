{
  "name": "electron-edge-js-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application using `electron-edge-js` with .NET",
  "main": "main.js",
  "scripts": {
    "start": "electron --core .",
    "start:core": "electron --core .",
    "start:standard": "electron --standard .",
    "start:native": "electron --native .",
    "rebuild:release": "node-gyp rebuild --msvs_version=2019 --target=11.4.3 --arch=x64 -release --dist-url=https://electronjs.org/headers/ --arch=x64 --directory=./node_modules/electron-edge-js",
    "rebuild:debug": "node-gyp rebuild --msvs_version=2019 --target=11.4.3 --arch=x64 -debug --dist-url=https://electronjs.org/headers/ --arch=x64 --directory=./node_modules/electron-edge-js"
  },
  "repository": "https://github.com/agracio/electron-edge-js-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "Edge",
    "Edge.js"
  ],
  "license": "MIT",
  "dependencies": {
    "electron": "^11.4.3",
    "electron-edge-js": "ArticySoftware/electron-edge-js"
  }
}
